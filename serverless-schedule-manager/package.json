{
  "name": "schedule-manager",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "echo 'Tests to come'; exit 1",
    "start": "twilio serverless:start",
    "deploy": "npm run fetch-config && twilio serverless:deploy --override-existing-project --runtime node16",
    "install-serverless-plugin": "twilio plugins:install @twilio-labs/plugin-serverless@v2",
    "deploy:dev": "npm run deploy -- --env .env.dev",
    "deploy:test": "npm run deploy -- --env .env.test",
    "deploy:qa": "npm run deploy -- --env .env.qa",
    "deploy:prod": "npm run deploy -- --env .env.prod",
    "fetch-config": "node scripts/fetch-config.js assets/config.private.json",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- --output-file eslint_report.json --format json"
  },
  "dependencies": {
    "@twilio/runtime-handler": "1.2.5",
    "axios": "^0.27.2",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.0.1",
    "rrule": "^2.7.1",
    "twilio": "^3.82.0",
    "twilio-flex-token-validator": "^1.5.6"
  },
  "devDependencies": {
    "eslint": "^8.37.0",
    "eslint-config-twilio": "^2.0.0",
    "twilio-cli": "^5.2.0",
    "twilio-run": "^3.4.2"
  },
  "engines": {
    "node": "16"
  }
}
